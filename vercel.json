{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "functions": {
    "app/**/*.tsx": {
      "runtime": "@vercel/next"
    }
  },
  "routes": [
    {
      "src": "/videos",
      "dest": "/videos"
    },
    {
      "src": "/calories",
      "dest": "/calories"
    },
    {
      "src": "/feedback",
      "dest": "/feedback"
    },
    {
      "src": "/learn",
      "dest": "/learn"
    },
    {
      "src": "/quiz",
      "dest": "/quiz"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/public/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
